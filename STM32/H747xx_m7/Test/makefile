SOURCES = Test.cpp

# ===== Macros ==============================================================

OBJECTS = $(SOURCES:.cpp=.o)

# ===== Rules ===============================================================

.cpp.o:
	$(GPP) -c $(CFLAGS) -o $@ $(INCLUDES) $<

# ===== Targets =============================================================

$(OUTPUT): Startup.o $(OBJECTS) $(LIBRARIES)
	$(GPP) $(CFLAGS) -T"$(KMS_LINKER_SCRIPTS)/STM32H747XX_M7_FLASH.ld" -Wl,-Map="Test.map" -o $@ $^
	$(SIZE) $@

# ===== Specific rules ======================================================

Startup.o: $(KMS_STARTUP)/startup_stm32H747xx_m7.s
	$(AS) -c $(CFLAGS) -o $@ $<

