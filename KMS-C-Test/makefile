include ../Common.mk

OUTPUT = ../Binaries/$(CONFIG)_$(PROCESSOR)/KMS-C-Test

LIBRARIES = $(KMS_A_A)

SOURCES = KMS-C-Test.cpp

# ===== Rules ===============================================================

.cpp.o:
	g++ -c $(CFLAGS) -o $@ $(INCLUDES) $<

# ===== Macros ==============================================================

OBJECTS = $(SOURCES:.cpp=.o)

# ===== Targets =============================================================

$(OUTPUT) : $(OBJECTS) $(LIBRARIES)
	g++ -o $@ $^

depend:
	makedepend -T $(INCLUDES) $(SOURCES)

